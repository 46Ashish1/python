def factorial(n):
 if n <= 1:
 return 1
 return n*factorial(n-1)
def fibonacci(n):
 if n <= 0:
 return 0
 if n == 1:
 return 1
 return fibonacci(n-1) + fibonacci(n-2)
if __name__ == "__main__":
 num = int(input("Enter a number: "))
 print(num,"th","term of fibonacci series: ".format(num),
fibonacci(num))
 print("Factorial of",num,"is", factorial(num))
